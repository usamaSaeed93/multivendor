{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": false,
        "jsx": "preserve",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noImplicitThis": false,
        "importHelpers": true,
        "allowSyntheticDefaultImports": true,
        "noUnusedLocals": false,        // changed to false
        "noUnusedParameters": false,     // changed to false
        "allowJs": true,                 // added
        "checkJs": false,               // added
        "skipLibCheck": true,           // added
        "noEmit": false,                // added
        "suppressImplicitAnyIndexErrors": true, // added
        "ignoreDeprecations": "5.0",    // added
        "any": true                     // added
    },
    "include": [
        "resources/js/**/*"
    ]
}